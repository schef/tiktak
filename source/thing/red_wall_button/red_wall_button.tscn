[sub_resource type="GDScript" id="GDScript_jbrx5"]
script/source = "extends Thing

const ANIM := {
	OFF = \"off\",
	ON = \"on\",
	TOGGLE = \"toggle\"
}

func _ready():
	super._ready()
	$AnimationPlayer.animation_finished.connect(_on_animation_player_animation_finished)

func closed():
	visible = false
	
func opening():
	visible = true
	animation.play(\"opening\")
	description = \"A strange opening WATA door!\"

func opened():
	animation.play(\"opened\")
	description = \"A strange WATA door!\"

func is_open():
	return animation.current_animation == \"opened\"
"

